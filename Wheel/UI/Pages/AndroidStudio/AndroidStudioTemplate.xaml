<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Wheel.UI.AndroidStudioTemplate"
             Title="AndroidStudioTemplate"
             xmlns:local="clr-namespace:Wheel.UI.Views">
    <ScrollView>
        <VerticalStackLayout>
            <Label x:Name="ProjectName" 
            Text="Current Project: Unknown"
            VerticalOptions="Center"
            HorizontalOptions="Center" FontSize="Large"/>

            <local:LoadFolderView x:Name="UploadProjectXml"/>

            <VerticalStackLayout x:Name="EntriesStackLayout" Margin="10">

            </VerticalStackLayout>
        </VerticalStackLayout>

    </ScrollView>
</ContentPage>